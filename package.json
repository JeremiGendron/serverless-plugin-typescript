  },
  "devDependencies": {
    "@types/fs-extra": "8.1.0",
    "@types/jest": "24.0.12",
    "@types/lodash": "4.14.149",
    "@types/node": "13.7.4",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsdoc": "^21.0.0",
    "eslint-plugin-prefer-arrow": "^1.1.7",
    "jest": "25.1.0",
    "mock-fs": "4.10.4",
    "rimraf": "3.0.2",
    "ts-jest": "25.2.1",
    "typescript": "^3.4.1"
  },
  "dependencies": {
    "fs-extra": "^8.1.0",
